<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.25.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="25" author="admin">
        <insert schemaName="sedec_sipaflore_mt" tableName="tb_tipo_doc_autorizacao">
            <column name="descricao" value="Procuração"/>
            <column name="codigo" value="PROCURACAO"/>
            <column name="descricao_longa" value="Procuração com poderes específicos para representar o requerente junto à SEDEC, com firma reconhecida;"/>
        </insert>
        <insert schemaName="sedec_sipaflore_mt" tableName="tb_tipo_doc_autorizacao">
            <column name="descricao" value="Procuração simples para advogado"/>
            <column name="codigo" value="PROCURACAO_SIMPLES_ADVOGADO"/>
