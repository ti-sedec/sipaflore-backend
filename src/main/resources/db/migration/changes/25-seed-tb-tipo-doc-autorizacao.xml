        </insert>
        <insert schemaName="sedec_sipaflore_mt" tableName="tb_tipo_doc_autorizacao">
            <column name="descricao" value="Documentos do representante do espólio (inventariante)"/>
            <column name="codigo" value="DOCUMENTO_REPRESENTANTE_ESPOLIO"/>
        </insert>
        <insert schemaName="sedec_sipaflore_mt" tableName="tb_tipo_doc_autorizacao">
